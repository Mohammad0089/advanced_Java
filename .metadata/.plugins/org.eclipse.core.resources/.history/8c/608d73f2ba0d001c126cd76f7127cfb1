package functional_Programming_with_Java;

import java.util.function.BiFunction;

public class Passing_function_as_argument {
	protected static class Mymath{
		
		public static Integer add(Integer x, Integer y) {
			return x + y;
		}
		protected static Integer subtract(Integer x, Integer y) {
			return x - y;
		}
		
		
		public static Integer combin2and3(BiFunction<Integer ,Integer ,Integer> combinFunc ) {
			return combinFunc.apply(2, 3);	
		}
	}
	public static void main (String [] args) {
		System.out.println(Mymath.combin2and3(Mymath::add));
		System.out.println(Mymath.combin2and3(Mymath::subtract));
		System.out.println(Mymath.combin2and3((x,y)-> x*2 + y*0));
	}
}
